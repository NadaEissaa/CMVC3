@model Department
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete Page</h1>


<br />
<form asp-action="Delete" method="post">

    <div class="my-3">
        <label asp-for="Code" class="col-form-label">Code</label>
        <input asp-for="Code" class="form-control"readonly />
    </div>

    <div class="my-3">
        <label asp-for="Name" class="col-form-label">Name</label>
        <input asp-for="Name" class="form-control" readonly />
    </div>

    <div class="my-3">
        <label asp-for="CreateAt" class="col-form-label">CreateAt</label>
        <input asp-for="CreateAt" type="date" class="form-control" readonly />
    </div>

    <div class="mt-3">
        <input type="submit" value="Delete" class="btn btn-danger" />
        <a asp-action="Index" class="btn btn-light">Back To List</a>
    </div>

</form>